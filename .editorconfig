#.editorconfig

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.py]
indent-style = space
indent_size = 4
max_line_length = 80

[*.{js,css,scss,html,json,jsx,md}]
indent-style = space
indent_size = 2

[**.min.js]
indent_style = ignore
insert_final_newline = ignore
